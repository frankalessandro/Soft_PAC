---
import { Image } from "astro:assets";
import imagenStiven from "../assets/StivenPic1.jpeg";
import imagenAlejo from "../assets/alejo_p.jpeg";
import imagenFrank from "../assets/FrankPic2.jpeg";
---

<div class="bg-white">
    <div
        class="flex flex-col items-center justify-center min-h-screen bg-gray-100"
    >
        <div class="w-full bg-white p-6 rounded-lg shadow-lg">
           

            <div class="container mx-auto py-8">
                <div class="flex flex-wrap justify-center gap-8">
                    <!-- Tarjeta 1 -->
                    <div
                        class="relative w-full max-w-72 flex flex-col justify-center items-center bg-white border border-gray-200 rounded-lg shadow-lg dark:bg-gray-800 dark:border-gray-700 mt-4 transition duration-300 transform hover:scale-105"
                    >
                        <div
                            class="flex flex-col items-center p-5 gap-4"
                        >
                            <Image
                                class="w-40 h-40 rounded-full shadow-xl object-cover"
                                src={imagenStiven}
                                alt="Imagen de Stiven Colorado"
                                loading="eager"
                            />
                            <span
                                class="text-xl font-semibold text-gray-900 dark:text-white"
                            >
                                Stiven Colorado
                            </span>
                            <span
                                class="text-md font-light text-gray-500 dark:text-gray-400"
                                >stivenchoo@gmail.com</span
                            >
                            <span
                                class="text-sm font-semibold text-green dark:text-gray-400"
                                >Software Dev</span
                            >
                            <div class="flex gap-5">
                                <a
                                    href="https://www.linkedin.com/in/stiven-colorado-370028220/"
                                    class="inline-flex items-center p-1 text-sm font-medium text-center text-white bg-green rounded-md hover:bg-green focus:ring-4 focus:outline-none focus:ring-green dark:bg-green dark:hover:bg-green dark:focus:ring-green"
                                >
                                    <svg
                                        class="w-10 h-10 text-white dark:text-white"
                                        aria-hidden="true"
                                        xmlns="http://www.w3.org/2000/svg"
                                        fill="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            fill-rule="evenodd"
                                            d="M12.5 8.8v1.7a3.7 3.7 0 0 1 3.3-1.7c3.5 0 4.2 2.2 4.2 5v5.7h-3.2v-5c0-1.3-.2-2.8-2.1-2.8-1.9 0-2.2 1.3-2.2 2.6v5.2H9.3V8.8h3.2ZM7.2 6.1a1.6 1.6 0 0 1-2 1.6 1.6 1.6 0 0 1-1-2.2A1.6 1.6 0 0 1 6.6 5c.3.3.5.7.5 1.1Z"
                                            clip-rule="evenodd"></path>
                                        <path
                                            d="M7.2 8.8H4v10.7h3.2V8.8Z"
                                        ></path>
                                    </svg>
                                </a>
                                <a
                                    href="https://github.com/StivenColorado"
                                    class="inline-flex items-center p-1 text-sm font-medium text-center text-white bg-green rounded-md hover:bg-green focus:ring-4 focus:outline-none focus:ring-green dark:bg-green dark:hover:bg-green dark:focus:ring-green"
                                >
                                    <svg
                                        class="w-10 h-10 dark:text-white"
                                        aria-hidden="true"
                                        xmlns="http://www.w3.org/2000/svg"
                                        fill="currentColor"
                                        viewBox="0 0 24 24"
                                    >
                                        <path
                                            fill-rule="evenodd"
                                            d="M12 2c-2.4 0-4.7.9-6.5 2.4a10.5 10.5 0 0 0-2 13.1A10 10 0 0 0 8.7 22c.5 0 .7-.2.7-.5v-2c-2.8.7-3.4-1.1-3.4-1.1-.1-.6-.5-1.2-1-1.5-1-.7 0-.7 0-.7a2 2 0 0 1 1.5 1.1 2.2 2.2 0 0 0 1.3 1 2 2 0 0 0 1.6-.1c0-.6.3-1 .7-1.4-2.2-.3-4.6-1.2-4.6-5 0-1.1.4-2 1-2.8a4 4 0 0 1 .2-2.7s.8-.3 2.7 1c1.6-.5 3.4-.5 5 0 2-1.3 2.8-1 2.8-1 .3.8.4 1.8 0 2.7a4 4 0 0 1 1 2.7c0 4-2.3 4.8-4.5 5a2.5 2.5 0 0 1 .7 2v2.8c0 .3.2.6.7.5a10 10 0 0 0 5.4-4.4 10.5 10.5 0 0 0-2.1-13.2A9.8 9.8 0 0 0 12 2Z"
