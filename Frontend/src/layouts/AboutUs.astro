                                                        aria-hidden="true"
                                                        focusable="false"
                                                        data-prefix="fab"
                                                        class="w-6 mr-4"
                                                        role="img"
                                                        xmlns="http://www.w3.org/2000/svg"
                                                        viewBox="0 0 512 512"
